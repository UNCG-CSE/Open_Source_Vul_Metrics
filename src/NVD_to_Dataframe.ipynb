    "import glob\n",
    "import pandas as pd\n",
    "\n",
    "path = '../data/nvd-hashs/'\n",
    "allcves = glob.glob(path+'*.csv')\n",
    "l = []\n",
    "\n",
    "for filename in allcves:\n",
    "    df = pd.read_csv(filename)\n",
    "    l.append(df)\n",
    "df_NVD_hash = pd.concat(l, ignore_index=True)\n",
    "#New cell\n",
    "df_NVD_hash"
   ]
