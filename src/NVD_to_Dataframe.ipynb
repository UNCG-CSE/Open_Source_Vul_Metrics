    "This function parses the NVD website for the _Base Score_ and _Description_ of each CVD ID"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "import bs4 as bs\n",
    "import urllib.request\n",
    "\n",
    "def getScore(df):\n",
    "    i = 0\n",
    "    while i < len(df['CVE']):\n",
    "        if i in [30, 61, 69]:\n",
    "            i += 1\n",
    "        else:\n",
    "            cve = df['CVE'][i]\n",
    "            url = 'https://nvd.nist.gov/vuln/detail/' + cve\n",
    "            source = urllib.request.urlopen(url).read()\n",
    "            soup = bs.BeautifulSoup(source,'html.parser')\n",
    "    \n",
    "            if soup.find_all('h2')[1].get_text() != 'CVE ID Not Found':\n",
    "                score = soup.find('span', attrs={'data-testid' : 'vuln-cvssv2-base-score'}).text\n",
    "                desc = soup.find('p', attrs={'data-testid' : 'vuln-description'}).text\n",
    "        \n",
    "                df['Score'][i] = score\n",
    "                df['Description'][i] = desc\n",
    "            i += 1\n",
    "    return df"
   ]
  },
